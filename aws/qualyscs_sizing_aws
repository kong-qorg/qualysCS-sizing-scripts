# :command.run
run() {
  declare -A args=()
  declare -a other_args=()
  declare -a input=()
  normalize_input "$@"
  parse_requirements "${input[@]}"

  case "$action" in
    "generate")
      if [[ ${args[--help]:-} ]]; then
        long_usage=yes
        qualyscs_sizing_aws_generate_usage
      else
        qualyscs_sizing_aws_generate_command
      fi
      ;;

    "root")
      root_command
      ;;

  esac
}

